    <!-- main query: updated table/column names and aliases -->
    <queryString>
        <![CDATA[
SELECT
    r.id AS id,
    CONCAT(c.first_name, ' ', c.last_name) AS clientName,
    CONCAT(uConsult.first_name, ' ', uConsult.last_name) AS consultantName,
    CONCAT(uSuper.first_name, ' ', uSuper.last_name) AS supervisorName,
    rs.description AS stateDescription,
    r.request_id AS requestId,
    r.installation_id AS installationId,
    r.cart_id AS cartId,
    r.observation AS observation
FROM ruc10_records r
JOIN clients c ON r.client_dni = c.dni
JOIN users uConsult ON r.consultant_user_id = uConsult.id
JOIN users uSuper ON r.supervisor_user_id = uSuper.id
JOIN record_states rs ON rs.id = r.state_id
WHERE ($P{id} = 0 OR r.state_id = $P{id})
ORDER BY id DESC
]]>
    </queryString>

